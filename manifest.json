{
  "name": "Wikidata Connect",
  "version": "0.4",
  "icons": {
   "200": "Cross_graph_7_Nodes.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
	"default_popup": "popup.html",
    "default_title": "Wikidata Connect"
  },
  //"background": {
  //   "scripts": ["background.js"]
  //},
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://maxcdn.bootstrapcdn.com; object-src 'self'",
  "permissions": ["tabs"]
}